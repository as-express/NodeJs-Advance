#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/as-express/Desktop/advance/jwt-cookie/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.1_ts-node@10.9.2_@types+node@22.8.1_typescript@5.6.3_/node_modules/typeorm/node_modules:/Users/as-express/Desktop/advance/jwt-cookie/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.1_ts-node@10.9.2_@types+node@22.8.1_typescript@5.6.3_/node_modules:/Users/as-express/Desktop/advance/jwt-cookie/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/as-express/Desktop/advance/jwt-cookie/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.1_ts-node@10.9.2_@types+node@22.8.1_typescript@5.6.3_/node_modules/typeorm/node_modules:/Users/as-express/Desktop/advance/jwt-cookie/node_modules/.pnpm/typeorm@0.3.20_pg@8.13.1_ts-node@10.9.2_@types+node@22.8.1_typescript@5.6.3_/node_modules:/Users/as-express/Desktop/advance/jwt-cookie/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-esm.js" "$@"
fi
